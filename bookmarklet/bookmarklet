javascript:(async()=>{var a=document.querySelectorAll('div[class="h3 my-0"] a[href*="endpoints"]');Array.prototype.forEach.call(a,(async function(a,e){var s=a.href.split("/");let t=`https://129.114.109.109:5000/caws?endpoint=${s.pop()||s.pop()}`;const l=await fetch(t),d=await l.json();var c=a.parentNode.parentNode.nextElementSibling.firstElementChild;if(null!=(o=c.querySelector(".caws-data")))o.innerHTML=d.total_energy;else{var i=document.createElement("div");i.classList.add("caws"),i.classList.add("col-lg"),i.classList.add("py-1");var n=document.createElement("div");n.classList.add("row");var r=document.createElement("div");r.classList.add("col-4"),r.classList.add("col-lg-12"),r.classList.add("small"),r.classList.add("text-uppercase"),r.classList.add("text-success"),r.innerHTML='Total energy expenditure (kWh) <img src="https://www.svgrepo.com/show/923/leaf.svg" alt="leaf" width="13px" heigh="13px" />',n.appendChild(r);var o=document.createElement("div");o.classList.add("caws-data"),o.classList.add("col-8"),o.classList.add("col-lg-12"),o.classList.add("small"),o.innerHTML=d.total_energy,n.appendChild(o),i.appendChild(n),c.appendChild(i)}}),a)})();
